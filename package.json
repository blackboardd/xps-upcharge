{
  "name": "xpsupcharge",
  "description": "Userscript that modifies XPS Shipper.",
  "version": "2.0.0",
  "main": "",
  "scripts": {
    "postversion": "git push --follow-tags",
    "analize": "npm_config_report=true npm run build",
    "build": "webpack --mode production --config config/webpack.config.production.cjs",
    "dev": "webpack --mode development --config config/webpack.config.dev.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:blackboardd/xps-upcharge.git"
  },
  "bugs": {
    "url": "https://github.com/blackboardd/xps-upcharge/issues"
  },
  "author": "blackboardd <86866786+blackboardd@users.noreply.github.com>",
  "license": "MIT",
  "homepage": "https://github.com/blackboardd/xps-upcharge#readme",
  "husky": {
    "hooks": {
      "prepare": "husky install",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/greasemonkey": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-loader": "^8.2.3",
    "browserslist": "^4.19.1",
    "commitizen": "^4.2.4",
    "css-loader": "^6.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "style-loader": "^3.3.1",
    "userscript-metadata-webpack-plugin": "^0.1.1",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "ansi-regex": "5.0.1",
    "axios": "^0.25.0",
    "axios-userscript-adapter": "^0.1.10",
    "jquery": "^3.6.0"
  }
}
